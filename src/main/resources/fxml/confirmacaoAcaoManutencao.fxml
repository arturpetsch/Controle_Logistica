<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="175.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.cl.controle_logistica.ConfirmacaoAcaoManutencaoController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Confirmar Ação?" textFill="#2196f3">
         <font>
            <Font name="Source Sans Pro Semibold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="33.0" prefHeight="6.0" prefWidth="350.0" style="-fx-background-color: #2196f3;">
         <font>
            <Font size="1.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="60.0" text="Data Manutenção :" />
      <Label layoutX="80.0" layoutY="93.0" text="Valor :" />
      <Button fx:id="botaoSim" layoutX="204.0" layoutY="135.0" mnemonicParsing="false" onAction="#confirmarAcao" prefHeight="25.0" prefWidth="52.0" text="Sim" />
      <Button fx:id="botaoNao" layoutX="114.0" layoutY="135.0" mnemonicParsing="false" onAction="#cancelarAcao" prefHeight="25.0" prefWidth="52.0" text="Não" />
      <Label fx:id="labelData" layoutX="128.0" layoutY="60.0" text="Label" />
      <Label fx:id="labelValor" layoutX="128.0" layoutY="93.0" text="Label" />
   </children>
</AnchorPane>
